{
  "headers": [
    {
      "source": "/multi-thread",
      "headers": [
        {
          "key": "Cross-Origin-Opener-Policy",
          "value": "same-origin"
        },
        {
          "key": "Cross-Origin-Embedder-Policy",
          "value": "require-corp"
        }
      ]
    }
  ],
  "rewrites": [
    { "source": "/single-thread", "destination": "/index.html" },
    { "source": "/multi-thread", "destination": "/index.html" }
  ]
}
